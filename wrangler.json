{
  "main": ".open-next/worker.js",
  "name": "opendots_alphav1",
  "compatibility_date": "2025-03-01",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  },
  "r2_buckets": [
    {
      "binding": "PROFILE_IMAGES",
      "bucket_name": "profile-images",
      "preview_bucket_name": "profile-images-dev"
    }
  ],
  "workers": [
    {
      "name": "image-worker",
      "main": "functions/image-worker.js",
      "routes": [
        {
          "pattern": "images/*",
          "custom_domain": false
        }
      ],
      "bindings": [
        {
          "name": "PROFILE_IMAGES",
          "type": "r2_bucket",
          "bucket_name": "profile-images"
        }
      ]
    }
  ]
}